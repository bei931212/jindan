}



/*玩法介绍*/
.game_rules{
	margin: 0.59rem 0.12rem 0;
	padding: 0 0.67rem;
	box-sizing: border-box;
	border:0.07rem solid #d63232;
	-webkit-border-radius: 15px;
	border-radius: 15px;
	background-color: #ffe6be;
	position: relative;
}
.game_rules .rules_content{
	font-size: 0.32rem;
