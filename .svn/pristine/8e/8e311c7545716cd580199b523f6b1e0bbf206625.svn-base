/*中奖弹窗*/
.win{
	position: fixed;
	width: 100%;
	height: 100%;
	left: 0;
	top: 0;
	background-color: rgba(0,0,0,.8);
	z-index: 100;
	display: flex;
	align-items: center;
	justify-content: center;
	display: none;
}
.win_content{
	position: absolute;
	width: 100%;
	height: 100%;
	left: 0;
	top: 0;
	display: flex;
	align-items: center;
	justify-content: center;
}
.flex_center{
	width: 6.13rem;
	position: relative;
}
.flex_center img{
	width: 100%;
}
.win .guang{
	width: 120%;
	/*height: auto;
	overflow: auto;*/
	-webkit-animation: 20s guang linear infinite;
	-moz-animation: 20s guang linear infinite;
	-ms-animation: 20s guang linear infinite;
	-o-animation: 20s guang linear infinite;
	animation: 20s guang linear infinite;
}
.win .win_content .flex_center div{
	position: absolute;
	text-align: center;
	width: 3.73rem;
	height: 3.07rem;
	left: 50%;
	margin-left: -1.865rem;
	bottom: 1.07rem;
}
.win .win_content .flex_center div p{
	font-size: 0.32rem;
	color:#fff;
	margin: 0.27rem 0 0;
	text-align: center;
}
.win .win_content .flex_center div span{
	font-size: 0.53rem;
	color:#f9d50f;
}
.win .win_content .flex_center div img{
	height: 0.87rem;
	width: 3.73rem;
	position: absolute;
	bottom: 0;
	left: 50%;
	margin-left: -1.865rem;
}


/*未中奖弹窗*/
.no_win{
	position: fixed;
	width: 100%;
	height: 100%;
	left: 0;
	top: 0;
	background-color: rgba(0,0,0,.8);
	z-index: 100;
	display: flex;
	align-items: center;
	justify-content: center;
	display: none;
}
.win_content{
	position: absolute;
	width: 100%;
	height: 100%;
	left: 0;
	top: 0;
	display: flex;
	align-items: center;
	justify-content: center;
}
.flex_center{
	width: 6.13rem;
	position: relative;
}
.flex_center img{
	width: 100%;
}
.no_win .guang{
	width: 100%;
	-webkit-transition: 5s guang;
	transition: 5s guang;
}
@-webkit-keyframes guang{
	0%{
		-webkit-transform: rotateZ(0deg);
		transform: rotateZ(0deg);
	}
	100%{
		-webkit-transform: rotateZ(360deg);
		transform: rotateZ(360deg);
	}
}
@-ms-keyframes guang{
	0%{
		-webkit-transform: rotateZ(0deg);
		transform: rotateZ(0deg);
	}
	100%{
		-webkit-transform: rotateZ(360deg);
		transform: rotateZ(360deg);
	}
}
@-moz-keyframes guang{
	0%{
		-webkit-transform: rotateZ(0deg);
		transform: rotateZ(0deg);
	}
	100%{
		-webkit-transform: rotateZ(360deg);
		transform: rotateZ(360deg);
	}
}
@keyframes guang{
	0%{
		-webkit-transform: rotateZ(0deg);
		transform: rotateZ(0deg);
	}
	100%{
		-webkit-transform: rotateZ(360deg);
		transform: rotateZ(360deg);
	}
}
.no_win .win_content .flex_center div{
	position: absolute;
	text-align: center;
	width: 3.73rem;
	left: 50%;
	margin-left: -1.865rem;
	bottom: 1.07rem;
}
.no_win .win_content .flex_center div p{
	font-size: 0.32rem;
	color:#fff;
	margin: 0.27rem 0 0.93rem;
}
.no_win .win_content .flex_center div span{
	font-size: 0.53rem;
	color:#f9d50f;
}
.no_win .win_content .flex_center div img{
	height: 0.87rem;
	width: 3.73rem;
}
.close_btn{
	display: block;
	width: 0.67rem;
	height: 0.67rem;
	position: absolute;
	top: 0;
	right: 0;
}



