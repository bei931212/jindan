}
.box .egg_left{
	position: absolute;
	left: 0.76rem;
}
.box .egg_center{
	position: absolute;
	left: 50%;
	margin-left: -1.275rem;
}
.box .egg_right{
	position: absolute;
	right: 0.72rem;
}
.box div.center{
	margin-top: 0.6rem;
}

.egg_mask{
	width:100vw;
	height: 100vh;
	position: fixed;
	left: 0;
	top: 0;
	background: rgba(0,0,0,.8);
	z-index: 40;
	display: none;
}



/*砸蛋有奖*/
.zadan_btn{
	height: 1.07rem;
	margin: 0.33rem auto 0;
}


/*获奖展示*/
.award_show{
