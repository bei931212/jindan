/*页面主体*/
.game_main{
	background: url("//statics.shunliandongli.com/images/big_bg.png?v=20170710") no-repeat;
	background-size: 100% auto;
	background-color: #f36f6f;
	padding-top: 4.53rem;
}

/*返回顶部*/
.backtop{
	width: 1.0rem;
	height: 1.0rem;
	position: fixed;
	right: 0.2rem;
	bottom: 2.27rem;
	display: none;
}

/*我的奖品、规则按钮*/
.fixed_btn{
	color:#fed2d2;
	font-size: 0.35rem;
	background: rgba(49,49,49,.5);
	padding: 0.07rem 0.3rem;
	border-radius: 10px;
	z-index: 51;
}
.btn_prize{
	position: fixed;
	left: 0.33rem;
	top: 0.33rem;
}
.btn_rules{
	position: fixed;
	right: 0.33rem;
	top: 0.33rem;
}

/*轮播*/
.swiper_box{
	margin: 0 auto;
	-webkit-border-radius: 10px;
	border-radius: 10px;
	border:3px solid #451111;
	width: 8.4rem!important;
	height: 1.99rem!important;
	background: #cbc6b6;
}
.game_main .swiper-pagination-bullet{
	width: 0.48rem;
	height: 0.05rem;
	background: rgba(255,255,255,.4);
}
.game_main .swiper-pagination-bullet-active{
	background: white;
}
.swiper_box .swiper-slide {
	overflow: hidden;
}
.swiper_box .swiper-slide a img{
	width: 100%;
}

/*砸蛋次数*/
.cishu{
	height: 0.8rem;
	width: 5rem;
	margin: 0.33rem auto 0;
	box-sizing: border-box;
	line-height: 0.9rem;
	font-size: 0.37rem;
	color: #fff45c;
	padding-left: 1.4rem;
	background: url("//statics.shunliandongli.com/images/cishu.png") no-repeat center;
	background-size:100%;
}

/*金蛋*/
.box{
	position: relative;
	/*top: 50%;*/
	margin-top: 0.07rem;
	padding: 0 0.73rem;
	box-sizing: border-box;
	/*height: 5.33rem;*/
	width: 100%;
	height: 4.13rem;
}
.box div{
	float: left;
	margin-right: 0.43rem;
