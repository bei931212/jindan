/*锤子*/
.chuizi{
	width: 3.5rem;
	height: 3.5rem;
	position: fixed;
	top: 30%;
	left: 60%;
	z-index: 101;
	display: none;
	-webkit-transform-origin: 50% 100%;
	-o-transform-origin: 50% 100%;
	-moz-transform-origin: 50% 100%;
	-ms-transform-origin: 50% 100%;
	transform-origin: 50% 100%;
}
.animation{
	display: block;
	-webkit-animation: move 2s forwards;
	-o-animation: move 2s forwards;
	-moz-animation: move 2s forwards;
	-ms-animation: move 2s forwards;
	animation: move 2s forwards;
}
@-webkit-keyframes move{
	0%{
		-webkit-transform: rotateZ(0deg);
		transform: rotateZ(0deg);
	}
	20%{
		-webkit-transform: rotateZ(30deg);
		transform: rotateZ(30deg);
	}
	35%{
		-webkit-transform: rotateZ(-50deg);
		transform: rotateZ(-50deg);
	}
	60%{
		-webkit-transform: rotateZ(30deg);
		transform: rotateZ(30deg);
	}
	70%{
		-webkit-transform: rotateZ(30deg);
		transform: rotateZ(30deg);
	}
	75%{
		-webkit-transform: rotateZ(-50deg);
		transform: rotateZ(-50deg);
	}
	100%{
		-webkit-transform: rotateZ(-50deg);
		transform: rotateZ(-50deg);
	}
}
@-moz-keyframes move{
	0%{
		-webkit-transform: rotateZ(0deg);
		transform: rotateZ(0deg);
	}
	20%{
		-webkit-transform: rotateZ(30deg);
		transform: rotateZ(30deg);
	}
	35%{
		-webkit-transform: rotateZ(-50deg);
		transform: rotateZ(-50deg);
	}
	60%{
		-webkit-transform: rotateZ(30deg);
		transform: rotateZ(30deg);
	}
	70%{
		-webkit-transform: rotateZ(30deg);
		transform: rotateZ(30deg);
	}
	75%{
		-webkit-transform: rotateZ(-50deg);
		transform: rotateZ(-50deg);
	}
	100%{
		-webkit-transform: rotateZ(-50deg);
		transform: rotateZ(-50deg);
	}
}
@-ms-keyframes move{
	0%{
		-webkit-transform: rotateZ(0deg);
		transform: rotateZ(0deg);
	}
	20%{
		-webkit-transform: rotateZ(30deg);
		transform: rotateZ(30deg);
	}
	35%{
		-webkit-transform: rotateZ(-50deg);
		transform: rotateZ(-50deg);
	}
	60%{
		-webkit-transform: rotateZ(30deg);
		transform: rotateZ(30deg);
	}
	70%{
		-webkit-transform: rotateZ(30deg);
		transform: rotateZ(30deg);
	}
	75%{
		-webkit-transform: rotateZ(-50deg);
		transform: rotateZ(-50deg);
	}
	100%{
		-webkit-transform: rotateZ(-50deg);
		transform: rotateZ(-50deg);
	}
}
@keyframes move{
	0%{
		-webkit-transform: rotateZ(0deg);
		transform: rotateZ(0deg);
	}
	20%{
		-webkit-transform: rotateZ(30deg);
		transform: rotateZ(30deg);
	}
	35%{
		-webkit-transform: rotateZ(-50deg);
		transform: rotateZ(-50deg);
	}
	60%{
		-webkit-transform: rotateZ(30deg);
		transform: rotateZ(30deg);
	}
	70%{
		-webkit-transform: rotateZ(30deg);
		transform: rotateZ(30deg);
	}
	75%{
		-webkit-transform: rotateZ(-50deg);
		transform: rotateZ(-50deg);
	}
	100%{
		-webkit-transform: rotateZ(-50deg);
		transform: rotateZ(-50deg);
	}
}

/*奖品展示*/
.prize{
	position: relative;
	margin: 0.59rem 0 0;
	background:#ad2727;
	padding-top: 1.2rem;
}
.prize .title_img{
	top: -0.20rem;
}
.prize .coupons{
	overflow: hidden;
	padding: 0 0.13rem;
}
.prize .coupons img{
	float: left;
	margin-right: 0.22rem;
	width: 2.27rem;
	height: 2.8rem;
}
.prize .coupons img:nth-child(4){
	margin-right: 0;
}
.prize_main{
	padding: 0 0.13rem;
	overflow: hidden;
	margin-top: 0.4rem;
}
.prize_main :nth-child(odd){
